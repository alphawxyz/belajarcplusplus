#include<iostream>
using namespace std; 

int main()
{
    cout<<"Perulangan While"<<endl;
    cout<<"\nContoh 1"<<endl;

    int a = 10;
    
    while (a < 20)
    {
        cout<<"Nilai A  : "<<a<<endl;
        a++;
    }

    cout<<"\nContoh 2"<<endl;
    int b = 12;
    while (b < 15)
    {
        cout<<"Nilai B  : "<<b<<endl;
        b++;
    }
    
    return 0;
}
