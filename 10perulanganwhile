#include<iostream>
using namespace std;

int main()
{
    cout<<"Perulangan Bersarang"<<endl;
    cout<<"While Bersarang"<<endl;
    int a,b = 10;

    while (a < 20)
    {
    while (b < 18)
    {
        cout<<"Nilai A  : "<<a<<endl;
        a++;
    }
        cout<<"Nilai B  : "<<b<<endl;
        b++;
    }

    return 0;
    
}
